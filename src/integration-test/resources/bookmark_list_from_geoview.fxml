<?xml version="1.0" encoding="UTF-8"?>

<?import com.esri.arcgisruntime.mapping.view.MapView?>
<?import com.esri.arcgisruntime.toolkit.BookmarkListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.StackPane?>
<?import com.esri.arcgisruntime.mapping.ArcGISMap?>
<fx:root type="StackPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <MapView fx:id="mapView">
        <map>
            <ArcGISMap/>
        </map>
    </MapView>
    <BookmarkListView StackPane.alignment="TOP_RIGHT" geoView="$mapView" maxWidth="150" maxHeight="300">
        <StackPane.margin>
            <Insets topRightBottomLeft="10"/>
        </StackPane.margin>
    </BookmarkListView>
</fx:root>
